    <script type="text/javascript" src="Uploader/demo.js"></script>
    
</body>


    <script type="text/javascript">
    //表单验证
        $(function(){
          $('#nextstep').click(function(){
            var txttitle = $('#txttitle').val();
            var addressdetail = $('#addressdetail').val();
            var txtname = $('#txtname').val();
            var txttel = $('#txttel').val();
            if(txttitle==""){
              $("#alarmbox-text").html("亲，标题还没填写！");
              $('#alarmbox').show();
              
            }
            else if(addressdetail==""){
              $("#alarmbox-text").html("亲，地址还没有填写！");
              $('#alarmbox').show();
              
            }
            else if(txttel.length!=11){
              $("#alarmbox-text").html("亲，手机号码长度错误！");
              $('#alarmbox').show();
              
            }
            else if(txtname==""){
              $("#alarmbox-text").html("亲，你的姓名还没有填写！");
              $('#alarmbox').show();
              
            }
            else{
                $('#userinfoform').submit();  
              }
          })
        });
    </script>

</html>